<Window x:Class="ConnectSQL.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConnectSQL"
        mc:Ignorable="d"
        Title="Window1" Height="200" Width="200" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Orientation="Vertical" VerticalAlignment="Center">
            <Button x:Name="btnCreateTable" Width="120" Height="20" Margin="10" Content="Create SQL Table" Click="btnCreateTable_Click"/>
            <Button x:Name="btnExport" Width="120" Height="20" Margin="10" Content="Export Data" Click="btnExport_Click"/>
            <Button x:Name="btnImport" Width="120" Height="20" Margin="10" Content="Import Data" Click="btnImport_Click"/>
        </StackPanel>
    </Grid>
</Window>
